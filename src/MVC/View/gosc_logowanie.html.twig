{% extends 'MVC/View/Layouts/login.html.twig' %}

{% block target %}
    /gosc_logowanie
{% endblock %}

{% block form %}

    <input class="yI" type="text" name="imie" placeholder="Imię" value="{{ imie }}" autocomplete="off" required><br/>

    <input class="yI" type="number" name="l_os" placeholder="Liczba osób" value="{{ l_os }}" autocomplete="off" required><br/>

    <input class="yI" type="text" name="k_stol" placeholder="Kod stolika" value="{{ k_stol }}" autocomplete="off" required><br/>

{% endblock %}

